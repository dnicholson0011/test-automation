<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="WARN" xmlns="http://logging.apache.org/log4j/2.0/config">
    <Appenders>
        <Console name="ConsoleAppender" target="SYSTEM_OUT">
            <PatternLayout pattern="[%t] %d{HH:mm:ss,SSS} %-p5 [%c{1}] %m%n"></PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="TRACE">
            <AppenderRef ref="ConsoleAppender"></AppenderRef>
        </Root>
    </Loggers>
</Configuration>